        <buttons>
            <Button cancelButton="true" mnemonicParsing="false" onAction="#cancel" text="Abbrechen" />
          <Button fx:id="btConfirm" defaultButton="true" mnemonicParsing="false" onAction="#confirm" text="Ãœbernehmen" />
        </buttons>
         <BorderPane.margin>
